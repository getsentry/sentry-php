includes:
    - phpstan-baseline.neon

parameters:
    tipsOfTheDay: false
    treatPhpDocTypesAsCertain: false
    level: 9
    paths:
        - src
    excludePaths:
        - tests/resources
        - tests/Fixtures
    dynamicConstantNames:
        - Monolog\Logger::API
    stubFiles:
        - stubs/ExcimerLog.stub
        - stubs/ExcimerLogEntry.stub
        - stubs/ExcimerProfiler.stub
        - stubs/ExcimerTimer.stub
        - stubs/globals.stub
